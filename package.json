{
  "name": "o-benchmarker",
  "version": "1.0.0",
  "description": "Node.JS benchmarking tool.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "SET NODE_ENV=test&& mocha --require ts-node/register tests/**/*.test.ts",
    "build": "tsc",
    "start": "ts-node ./src/index.ts",
    "prod": "npm run build && npm run start",
    "o-benchmarker": "ts-node ./lib/driver.ts",
    "benchmark": "npm run o-benchmarker **/*.benchmark.ts minfo",
    "o-benchmarker:dist": "ts-node ./dist/driver.js",
    "benchmark:dist": "npm run o-benchmarker:dist **/*.benchmark.ts minfo"
  },
  "bin" : {},
  "author": "Ofir G.",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^10.12.8",
    "colors": "^1.3.2",
    "glob": "^7.1.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "mocha": "^5.2.0"
  }
}
